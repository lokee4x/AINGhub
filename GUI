local player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui")
gui.Parent = player:WaitForChild("PlayerGui")

-- Main Frame
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 400, 0, 250)
mainFrame.Position = UDim2.new(0.3, 0, 0.3, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 40)
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.Parent = gui

-- Title
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, 0, 0, 30)
title.BackgroundColor3 = Color3.fromRGB(10, 10, 30)
title.Text = "AING"
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.Font = Enum.Font.SourceSansBold
title.TextSize = 20
title.Parent = mainFrame

-- Tabs Section
local tabsFrame = Instance.new("Frame")
tabsFrame.Size = UDim2.new(0, 100, 1, -30)
tabsFrame.Position = UDim2.new(0, 0, 0, 30)
tabsFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 30)
tabsFrame.Parent = mainFrame

-- Main Tab Button
local mainTab = Instance.new("TextButton")
mainTab.Size = UDim2.new(1, 0, 0, 40)
mainTab.BackgroundColor3 = Color3.fromRGB(50, 50, 200)
mainTab.Text = "Main"
mainTab.TextColor3 = Color3.fromRGB(255, 255, 255)
mainTab.Font = Enum.Font.SourceSansBold
mainTab.TextSize = 18
mainTab.Parent = tabsFrame

-- Other Tab Button
local otherTab = Instance.new("TextButton")
otherTab.Size = UDim2.new(1, 0, 0, 40)
otherTab.Position = UDim2.new(0, 0, 0, 40)
otherTab.BackgroundColor3 = Color3.fromRGB(100, 50, 200)
otherTab.Text = "Other"
otherTab.TextColor3 = Color3.fromRGB(255, 255, 255)
otherTab.Font = Enum.Font.SourceSansBold
otherTab.TextSize = 18
otherTab.Parent = tabsFrame

-- Main Tab Content
local mainContent = Instance.new("Frame")
mainContent.Size = UDim2.new(1, -100, 1, -30)
mainContent.Position = UDim2.new(0, 100, 0, 30)
mainContent.BackgroundTransparency = 1
mainContent.Parent = mainFrame

-- Other Tab Content
local otherContent = Instance.new("Frame")
otherContent.Size = UDim2.new(1, -100, 1, -30)
otherContent.Position = UDim2.new(0, 100, 0, 30)
otherContent.BackgroundTransparency = 1
otherContent.Visible = false
otherContent.Parent = mainFrame

-- Add Amount TextBox
local addAmount = Instance.new("TextBox")
addAmount.Size = UDim2.new(0.45, -10, 0, 40)
addAmount.Position = UDim2.new(0, 10, 0, 10)
addAmount.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
addAmount.Text = "$ $ $ $"
addAmount.TextColor3 = Color3.fromRGB(255, 255, 255)
addAmount.TextTransparency = 0.3
addAmount.Font = Enum.Font.SourceSansBold
addAmount.TextSize = 18
addAmount.Parent = mainContent

-- Add Money Button
local addMoney = Instance.new("TextButton")
addMoney.Size = UDim2.new(0.45, -10, 0, 40)
addMoney.Position = UDim2.new(0.5, 0, 0, 10)
addMoney.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
addMoney.Text = "Add Money"
addMoney.TextColor3 = Color3.fromRGB(255, 255, 255)
addMoney.Font = Enum.Font.SourceSansBold
addMoney.TextSize = 18
addMoney.Parent = mainContent

addMoney.MouseButton1Click:Connect(function()
    local amount = tonumber(addAmount.Text) or 0
    print("Adding Money: $" .. amount)
    -- Add money logic here (modify as per your game setup)
end)

-- Highlight Paths Button
local highlightPaths = Instance.new("TextButton")
highlightPaths.Size = UDim2.new(0.9, 0, 0, 40)
highlightPaths.Position = UDim2.new(0.05, 0, 0, 60)
highlightPaths.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
highlightPaths.Text = "Highlight Paths"
highlightPaths.TextColor3 = Color3.fromRGB(255, 255, 255)
highlightPaths.Font = Enum.Font.SourceSansBold
highlightPaths.TextSize = 18
highlightPaths.Parent = mainContent

highlightPaths.MouseButton1Click:Connect(function()
    for _, part in pairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") then
            part.BrickColor = BrickColor.new("Bright blue")
        end
    end
end)

-- Infinite Yield Button
local infiniteYield = Instance.new("TextButton")
infiniteYield.Size = UDim2.new(0.9, 0, 0, 40)
infiniteYield.Position = UDim2.new(0.05, 0, 0, 10)
infiniteYield.BackgroundColor3 = Color3.fromRGB(50, 50, 200)
infiniteYield.Text = "Infinite Yield"
infiniteYield.TextColor3 = Color3.fromRGB(255, 255, 255)
infiniteYield.Font = Enum.Font.SourceSansBold
infiniteYield.TextSize = 18
infiniteYield.Parent = otherContent

infiniteYield.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

-- Tab Switching Logic
mainTab.MouseButton1Click:Connect(function()
    mainContent.Visible = true
    otherContent.Visible = false
end)

otherTab.MouseButton1Click:Connect(function()
    mainContent.Visible = false
    otherContent.Visible = true
end)
